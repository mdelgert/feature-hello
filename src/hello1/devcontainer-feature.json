{
    // https://containers.dev/implementors/features/#devcontainer-feature-json-properties
    "name": "Hello, World!",
    "id": "hello1",
    "version": "1.0.0",
    "description": "A hello world feature",
    "options": {
        "featureVersion": {
          "type": "string",
          "default": "1.0.0"
        },
        "greeting": {
            "type": "string",
            "proposals": [
                "hey",
                "hello",
                "hi",
                "howdy"
            ],
            "default": "Hello1",
            "description": "Select a pre-made greeting, or enter your own"
        }
    }
    // "installsAfter": [
